{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "outDir": "dist",
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "types": ["unplugin-icons/types/react"],
        "allowImportingTsExtensions": true,
        "lib": ["ES2021", "DOM"]
    },
    "include": [".astro/types.d.ts", ".astro/env.d.ts", "**/*"],
    "exclude": ["playwright-report", "dist", "node_modules"]
}

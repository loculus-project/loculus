{
  "name": "pathoplexus-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "start-test-server": "astro build && astro preview --port 3001",
    "build": "astro check && tsc --noEmit && astro build",
    "build-netlify": "astro build --config astro.netlify.config.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "check-format": "eslint \"src/**/*.{ts,tsx,astro}\" && prettier --ignore-path \"../.gitignore\" --check \"{src,tests}/**/*.{ts,tsx,json,astro}\"",
    "format": "eslint \"src/**/*.{ts,tsx,astro}\" --fix && prettier --ignore-path \"../.gitignore\" --write \"{src,tests}/**/*.{ts,tsx,json,astro}\"",
    "test": "vitest --dir ./src",
    "e2e": "npx playwright test",
    "e2e:headed": "npx playwright test --headed"
  },
  "dependencies": {
    "@astrojs/netlify": "^2.5.1",
    "@astrojs/node": "^5.3.0",
    "@astrojs/react": "^2.2.1",
    "@astrojs/tailwind": "^4.0.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.3",
    "@mui/x-date-pickers": "^6.9.1",
    "@tanstack/react-query": "^4.32.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "astro": "^2.9.2",
    "change-case": "^4.1.2",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tanstack/eslint-plugin-query": "^4.29.25",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "cross-env": "^7.0.3",
    "daisyui": "^3.2.1",
    "eslint": "^8.46.0",
    "eslint-plugin-astro": "^0.28.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "prettier-plugin-astro": "^0.11.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}

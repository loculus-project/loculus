        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@tanstack/react-query": "^4.36.1",
        "@zodios/core": "^10.9.6",
        "@zodios/react": "^10.4.5",
        "astro": "^5.0.2",
        "axios": "^1.7.9",
        "change-case": "~5.3.0",
        "chart.js": "^4.4.7",

        "just-kebab-case": "^4.2.0",
        "jwks-rsa": "^3.1.0",
        "luxon": "^3.5.0",
        "neverthrow": "^8.1.1",
        "openid-client": "^5.7.1",
        "react": "^18.3.1",
        "react-chartjs-2": "^5.2.0",
        "react-confirm-alert": "^3.0.6",
        "react-dom": "^18.3.1",
        "react-toastify": "^10.0.6",
        "react-tooltip": "^5.28.0",
        "rsuite": "^5.75.0",
        "unplugin-icons": "^0.21.0",
